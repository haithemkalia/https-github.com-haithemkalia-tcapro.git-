{
  "name": "tca-visa-tracking-system",
  "version": "1.0.0",
  "description": "TCA Visa Tracking System - شركة تونس للاستشارات والخدمات",
  "main": "app.py",
  "scripts": {
    "build": "webpack --mode production --output-path public",
    "dev": "webpack --mode development --watch",
    "start": "python app.py",
    "build:css": "postcss static/css/custom.css -o public/css/custom.min.css --use autoprefixer cssnano",
    "build:js": "webpack --entry ./static/js/custom.js --entry ./static/js/dashboard.js --output-path public/js",
    "build:static": "npm run build:css && npm run build:js",
    "clean": "rimraf public",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "visa",
    "tracking",
    "flask",
    "tunisia",
    "tca",
    "consulting"
  ],
  "author": "TCA - Tunisia Consulting and Services",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.23.6",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.34.0",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "postcss-loader": "^7.3.3",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@vercel/speed-insights": "^1.2.0",
    "bootstrap": "^5.3.2",
    "jquery": "^3.7.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tca-consulting/visa-tracking-system.git"
  },
  "bugs": {
    "url": "https://github.com/tca-consulting/visa-tracking-system/issues"
  },
  "homepage": "https://github.com/tca-consulting/visa-tracking-system#readme"
}
